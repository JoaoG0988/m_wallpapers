{
  "name": "m_wallpapers",
  "version": "1.0.0",
  "description": "wallpapers collection that i have used on my computer.",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && cd wallpapers-frontend && npm install",
    "start": "tsx watch src/server.ts",
    "backend": "npm run start",
    "frontend": "cd wallpapers-frontend && npm run dev",
    "start:all": "concurrently \"npm run backend\" \"npm run frontend\"",
    "docker:up": "docker compose up -d",
    "prisma:migrate": "npx prisma migrate dev",
    "dev:full": "npm run docker:up && npm run start:all",
    "stop": "docker compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoaoG0988/m_wallpapers.git"
  },
  "keywords": [],
  "author": "JoaoG0988",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/JoaoG0988/m_wallpapers/issues"
  },
  "homepage": "https://github.com/JoaoG0988/m_wallpapers#readme",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.19.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "lucide-react": "^0.562.0"
  }
}
